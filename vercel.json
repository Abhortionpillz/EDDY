{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "admin.html", "use": "@vercel/static" },
    { "src": "css/style.css", "use": "@vercel/static" },
    { "src": "js/app.js", "use": "@vercel/static" },
    { "src": "api/upload.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/upload.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
